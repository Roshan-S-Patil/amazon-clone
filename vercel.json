{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/client/public/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/server/index.js"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/old-route",
      "destination": "/new-route"
    },
    {
      "source": "/products/(.*)",
      "destination": "/product?id=$1"
    }
  ]
}